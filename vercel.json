{
  "rewrites": [
    {
      "source": "/blogg/:slug",
      "destination": "https://tpehwkimphnkjaxqnpjq.supabase.co/functions/v1/og-meta?slug=:slug",
      "has": [
        {
          "type": "query",
          "key": "share",
          "value": "1"
        }
      ]
    },
    {
      "source": "/blogg/:slug",
      "destination": "https://tpehwkimphnkjaxqnpjq.supabase.co/functions/v1/og-meta?slug=:slug",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|InstagramBot|Twitterbot|LinkedInBot|WhatsApp|TelegramBot|Slackbot|Pinterest|Discordbot).*"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

